    "eslint": "^2.13.1",
    "expose-loader": "^0.7.1",
    "webpack": "^4.16.3",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "control-panel": "^1.2.0",
    "gl-mat4": "^1.1.4",
